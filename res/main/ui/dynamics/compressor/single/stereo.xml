<plugin resizable="true">
	<vbox>
		<!-- Top part -->
		<grid rows="1" cols="5" hspacing="4" expand="true">
			<vbox expand="true">
				<!-- <group ui:inject="GraphGroup" ipadding="0" expand="true" fill="true" text="labels.graphs.time"> -->
					<graph width.min="496" height.min="222" expand="true" fill="true">
						<origin hpos="1" vpos="-1" visible="false"/>

						<ui:eval id="min_signal" value="-48 db"/>
						<ui:eval id="max_signal" value="6 db"/>

						<!-- Axis -->
						<axis min="0" max="5" angle="1.0" log="false" color="graph_prim" visibility="false"/>
						<!-- oy 0 -->
						<axis min="${min_signal}" max="${max_signal}" angle="0.5" log="true" color="graph_prim" visibility="false"/>

						<!-- Gray grid -->
						<ui:for id="t" first="0" last="4" step="1">
							<ui:attributes ox="0" oy="1">
								<marker v="${t}.25" color="graph_alt_2"/>
								<marker v="${t}.5" color="graph_alt_1"/>
								<marker v="${t}.75" color="graph_alt_2"/>
							</ui:attributes>
						</ui:for>
						<ui:attributes ox="1" oy="0" color="graph_alt_1">
							<marker v="+12 db"/>
							<marker v="-12 db"/>
							<marker v="-36 db"/>
							<marker v="-60 db"/>
						</ui:attributes>

						<!-- Yellow grid -->
						<ui:for id="t" first="1" last="4" step="1">
							<marker v="${t}" ox="0" oy="1" color="graph_sec"/>
						</ui:for>
						<ui:attributes ox="1" oy="0" color="graph_sec">
							<marker v="0 db"/>
							<marker v="-24 db"/>
							<marker v="-48 db"/>
						</ui:attributes>

						<!-- Meshes -->
						<!-- TODO: Show input signal if sidechain compression is used -->
						<!-- <mesh id="icg_l" width="2" color="left_in" visibility=":ilv_l"/>
						<mesh id="icg_r" width="2" color="right_in" visibility=":ilv_r"/> -->
						<!-- <mesh id="ocg_l" width="2" color="left" visibility=":olv_l"/>
						<mesh id="ocg_r" width="2" color="right" visibility=":olv_r"/> -->

						<!-- Input signal -->
						<mesh id="icg_l" ui:inject="GraphMesh_in" fill="true" visibility=":ilv_l" />

						<!-- Output signal -->
						<mesh id="ocg_l" ui:inject="GraphMesh_out" fill="true" visibility=":olv_l" />

						<!-- Input (Sidechain) signal -->
						<mesh id="scg_l" width="2" color="orange" fcolor="#444" color.a="0.5" fcolor.a="0.9" fill="true" visibility=":slv_r and :sct ieq 2"/>
						<mesh id="scg_r" width="2" color="orange" fcolor="#444" color.a="0.5" fcolor.a="0.7" fill="true" visibility=":slv_r and :sct ieq 2"/>

						<!-- Output signal -->
						<!-- <mesh id="ocg_l" width="2" color="#777" fcolor="#777" color.a="0.3" fcolor.a="0.9" fill="false" visibility=":olv_l" /> -->
						<!-- <mesh id="scg_r" width="0" fcolor="#666" fcolor.a="0.7" fill="true" visibility=":slv_r"/> -->

						<!-- Gain reduction/expansion -->
						<mesh id="grg_l" width="0" fill="true" fcolor="#993333" fcolor.a="0.5" visibility=":grv_l"/>
						<!-- <mesh id="grg_r" width="0" fill="true" fcolor="#bf993333" visibility=":grv_r"/> -->

						<!-- Compression detector curve -->
						<mesh id="evg_l" width="3" color="cyan" visibility=":elv_l"/>
						<!-- <mesh id="evg_r" width="3" color="cyan" visibility=":elv_r"/> -->
						<!-- TODO: Check this upstream changes -->
<!--
						<mesh id="icg_l" width="0" color="left_in" fcolor="left_in" fcolor.a="0.8" fill="true" visibility=":ilv_l"/>
						<mesh id="icg_r" width="0" color="right_in" fcolor="right_in" fcolor.a="0.8" fill="true" visibility=":ilv_r"/>
						<mesh id="ocg_l" width="2" color="left" visibility=":olv_l"/>
						<mesh id="ocg_r" width="2" color="right" visibility=":olv_r"/>
						<mesh id="scg_l" width="2" color="sidechain_1" visibility=":slv_l"/>
						<mesh id="scg_r" width="2" color="sidechain_2" visibility=":slv_r"/>
						<mesh id="evg_l" width="2" color="envelope_1" visibility=":elv_l"/>
						<mesh id="evg_r" width="2" color="envelope_2" visibility=":elv_r"/>
						<mesh id="grg_l" width="2" color="bright_blue" fcolor="bright_blue" fcolor.a="0.8" fill="true" visibility=":grv_l"/>
						<mesh id="grg_r" width="2" color="bright_blue" fcolor="bright_blue" fcolor.a="0.8" fill="true" visibility=":grv_r"/> -->
						<!-- TODO: /Check this upstream changes -->

						<!-- Boundaries -->
						<marker v="24 db" ox="1" oy="0" color="graph_prim" visibility="false"/>
						<marker v="5" ox="0" oy="1" color="graph_prim" visibility="false"/>

						<!-- Markers -->
						<ui:attributes ox="1" oy="0">
							<marker id="rl" width="2" pointer="vsize" color="release" lcolor="release" ox="1" oy="0" lborder="25" color.a="0.1" lcolor.a="0.8"/>
							<marker id="al" editable="true" width="3" pointer="vsize" color="threshold" hcolor="threshold" lcolor="threshold" hlcolor="threshold" ox="1" oy="0" lborder="20" hlborder="20" color.a="0.1" hcolor.a="0.0" lcolor.a="0.8" hlcolor.a="0.6"/>
						</ui:attributes>

						<!-- Text -->
						<text x="5" y="-72 db" text="graph.units.s" halign="1" valign="1" color="graph_prim"/>
						<ui:for id="t" first="0" last="4" step="1">
							<text x="${t}" y="-72 db" text="${t}" halign="-1" valign="1" color="graph_prim"/>
							<text x="${t}.5" y="-72 db" text="${t}.5" halign="-1" valign="1" color="graph_prim"/>
						</ui:for>
						<ui:attributes x="5" halign="1" valign="1" color="graph_prim">
							<text y="12 db" text="+12"/>
							<text y="0 db" text="0"/>
							<text y="-12 db" text="-12"/>
							<text y="-24 db" text="-24"/>
							<text y="-36 db" text="-36"/>
							<text y="-48 db" text="-48"/>
							<text y="-60 db" text="-60"/>
							<text x="5" y="24 db" text="graph.units.db" valign="-1"/>
						</ui:attributes>
					</graph>
				<!-- </group> -->
				<void bg.color="bg" vexpand="false" fill="false" pad.v="2"/>
				<hbox spacing="4" vexpand="false" bg.color="bg_schema" pad.h="6">
					<button id="pause" height="22" width="48" text="labels.pause" pad.v="4" ui:inject="Button_green"/>
					<button id="clear" height="22" width="48" text="labels.clear" pad.v="4" ui:inject="Button_red"/>
					<void hexpand="true"/>
					<vsep pad.h="2" bg.color="bg" reduce="true"/>
					<button id="ssplit" text="labels.stereo_split" size="22" pad.v="4" ui:inject="Button_blue"/>
				</hbox>
			</vbox>

			<grid rows="3" cols="3" hfill="false" hspacing="4" hexpand="false">
				<ui:with bg.color="bg_schema">
					<label text="labels.sc.SC" padding="4"/>
					<label text="labels.Env_elope" padding="4"/>
					<label text="labels.gain_" padding="4"/>
					<!-- <grid rows="2" cols="2" fill="false" hspacing="2">
						<button id="slv_l" size="10" ui:inject="Button_orange" led="true"/>
					</grid> -->
					<button id="slv_l" size="20" ui:inject="Button_orange" led="true"/>

					<!-- <grid rows="2" cols="2" fill="false" hspacing="2">
						<button id="elv_l" size="10" ui:inject="Button_violet" led="true"/>
						<button id="elv_r" size="10" ui:inject="Button_violet" led="true"/>
					</grid> -->
					<button id="elv_l" size="20" ui:inject="Button_cyan" led="true"/>

					<!-- <grid rows="2" cols="2" fill="false" hspacing="2">
						<button id="grv_l" size="10" ui:inject="Button_blue" led="true"/>
					</grid> -->
					<button id="grv_l" size="20" ui:inject="Button_red" led="true"/>
				</ui:with>

				<!-- Sidechain signal meter -->
				<ledmeter vexpand="true" height.min="256" angle="1">
					<ledchannel id="slm_l" min="${min_signal}" max="${max_signal}" log="true" type="peak" value.color="orange" /> <!-- activity=":slv_l" -->
					<!-- <ledchannel id="slm_r" min="${min_signal}" max="${max_signal}" log="true" type="peak" value.color="gray" /> -->
				</ledmeter>

				<!-- Envelope signal meter -->
				<ledmeter vexpand="true" height.min="256" angle="1">
					<ledchannel id="elm_l" min="${min_signal}" max="${max_signal}" log="true" type="peak" value.color="cyan"/> <!-- activity=":elv_l" -->
					<!-- <ledchannel id="elm_r" min="${min_signal}" max="${max_signal}" log="true" type="peak" value.color="cyan"/> -->
				</ledmeter>

				<!-- Gain signal meter -->
				<ledmeter vexpand="true" height.min="256" angle="1">
					<ledchannel id="rlm_l" min="-48 db" max="48 db" log="true" balance="0 db" balance.visibility="true" type="peak" value.color="red"/> <!-- activity=":grv_l" -->
					<!-- <ledchannel id="rlm_r" min="-48 db" max="48 db" log="true" balance="0 db" balance.visibility="true" type="peak" value.color="red"/> -->
				</ledmeter>
			</grid>

			<group ui:inject="GraphGroup" ipadding="0" expand="true" fill="true" text.show="false">
				<vbox>
					<!-- Top panel -->
					<hbox bg.color="bg_schema" fill="true">
						<align halign="-1">
							<label text="labels.graphs.compression"  />
						</align>

						<label text="labels.knee" vreduce="true"/>
						<knob id="kn" size="10" ssize="3" scolor="kscale"/>
						<value id="kn" step="-1" sline="true" width="64" />
					</hbox>

					<hbox>
						<!-- Curve graph -->
						<graph width.min="256" height.min="256" expand="true" fill="true">
							<origin hpos="-1" vpos="-1" visible="false"/>

							<!-- OX -->
							<axis min="-64 db" max="12 db" color="graph_prim" angle="0.0" log="true" visibility="false"/>
							<!-- OY -->
							<axis min="-64 db" max="12 db" color="graph_prim" angle="0.5" log="true" visibility="false"/>
							<!-- OY for ratio -->
							<!-- FIXME: Rewrite this formula -->
							<axis max="1" min="(:al ** 2.8 ) * 8000000000" color="graph_prim" angle="-0.5" log="true" visibility="false"/>
							<!-- 0 db 2000000000000 -->
							<!-- -12 db 2000000 -->
							<!-- -30 db 4000 -->
							<!-- -80 db 130 -->

							<!-- Gray grid -->
							<ui:for id="g" first="-60" last="12" step="24">
								<marker v="${g} db" color="graph_alt_1"/>
								<marker v="${g} db" ox="1" oy="0" color="graph_alt_1"/>
							</ui:for>

							<!-- Yellow grid -->
							<ui:for id="g" first="-48" last="-24" step="24">
								<marker v="${g} db" color="graph_sec"/>
								<marker v="${g} db" ox="1" oy="0" color="graph_sec"/>
							</ui:for>

							<!-- Axes and boundaries -->
							<marker v="1" ox="1" oy="0" dv="1" dx=":_a_width" dy=":_a_height" color="graph_alt_1" width="2" smooth="true"/>
							<marker v="24 db" color="graph_alt_1" visibility="false"/>
							<marker v="24 db" ox="1" oy="0" color="graph_alt_1" visibility="false"/>

							<!-- 0 dB markers -->
							<marker v="0 db" ox="0" oy="1" width="2" color="graph_prim"/>
							<marker v="0 db" ox="1" oy="0" width="2" color="graph_prim"/>

							<!-- Meshes -->
							<mesh id="ccg" width="3" color="graph_mesh" smooth="true"/>
							<dot x.id="elm_l" y.id="clm_l" color="graph_mesh" bcolor="graph_mesh" visibility="!:ssplit"/>
							<dot x.id="elm_l" y.id="clm_l" color="left" bcolor="left" visibility=":ssplit"/>
							<dot x.id="elm_r" y.id="clm_r" color="right" bcolor="right" visibility=":ssplit"/>

							<!-- Markers -->
							<marker id="al" editable="true" width="2" pointer="hsize" color="attack_1" hcolor="attack_1"/>
							<marker id="rl" color="release_1"/>

							<!-- Text -->
							<text x="0 db" y="0 db" text="0" halign="1" valign="1"/>
							<ui:attributes x="0 db" halign="1" valign="-1">
								<text y="24 db" text="graph.units.db"/>
								<text y="12 db" text="+12"/>
								<text y="-12 db" text="-12"/>
								<text y="-24 db" text="-24"/>
								<text y="-36 db" text="-36"/>
								<text y="-48 db" text="-48"/>
								<text y="-60 db" text="-60"/>
								<text y="-72 db" text="graph.axis.out" valign="1"/>
							</ui:attributes>
							<ui:attributes y="0 db" halign="-1" valign="1">
								<text x="24 db" text="graph.units.db"/>
								<text x="12 db" text="+12"/>
								<text x="-12 db" text="-12"/>
								<text x="-24 db" text="-24"/>
								<text x="-36 db" text="-36"/>
								<text x="-48 db" text="-48"/>
								<text x="-60 db" text="-60"/>
								<text x="-72 db" text="graph.axis.in" halign="1"/>
							</ui:attributes>

							<!-- Ratio dot -->
							<!-- <marker id="cr" min="1" max="100" ox="2" oy="0" editable="true" pointer="vsize" color="cyan" width="2" hcolor="cyan" /> -->
							<!-- <dot x="9 db" y.id="cr" y.min="1" y.max="100" y.editable="true" size="7" hover.size="7" pointer="drag" oy="2" color="cyan" /> -->

							<!-- Compression rate -->
							<!-- <text x="-10 db" y="-60 db" text="${cr}:1" text.eval="true" size="18" halign="1" font.bold="true" valign="1" color="cyan" /> -->
						</graph>

						<!-- <fader id="cr" vexpand="true" angle="1" ui:inject="Fader_prophessor_ratio" min="100" max="1" step="-0.0025" /> -->
					</hbox>
				</vbox>
			</group>

			<!-- Signal meters -->
			<hbox>
				<!-- Input -->
				<vbox>
					<ui:with bg.color="bg_schema">
						<label text="labels.chan.in" padding="4" />
					</ui:with>

					<hbox>
						<fader id="g_in" vexpand="true" angle="1" ui:inject="Fader_prophessor" />

						<vbox>
							<button id="ilv_l" size="20" ui:inject="Button_gray" led="true"/>

							<ledmeter vexpand="true" height.min="256" angle="1">
								<ledchannel id="ilm_l" min="${min_signal}" max="${max_signal}" log="true" type="rms_peak" peak.visibility="true" activity=":ilv_l" value.color="left_in"/>
								<ledchannel id="ilm_r" min="${min_signal}" max="${max_signal}" log="true" type="rms_peak" peak.visibility="true" activity=":ilv_r" value.color="right_in"/>
							</ledmeter>
						</vbox>
					</hbox>
				</vbox>

				<!-- Output -->
				<vbox>
					<ui:with bg.color="bg_schema">
						<label text="labels.chan.out" padding="4" />
					</ui:with>

					<hbox>
						<fader id="g_out" vexpand="true" angle="1" ui:inject="Fader_prophessor" />

						<vbox>
							<button id="olv_l" size="20" ui:inject="Button_gray" led="true"/>

							<ledmeter vexpand="true" height.min="256" angle="1">
								<ledchannel id="olm_l" min="${min_signal}" max="${max_signal}" log="true" type="rms_peak" peak.visibility="true" activity=":olv_l" value.color="left"/>
								<ledchannel id="olm_r" min="${min_signal}" max="${max_signal}" log="true" type="rms_peak" peak.visibility="true" activity=":olv_r" value.color="right"/>
							</ledmeter>
						</vbox>

					</hbox>
				</vbox>

			</hbox>
		</grid>

		<!-- Separator -->
		<void bg.color="bg" fill="true" pad.v="2" reduce="true"/>

		<!-- Bottom part -->
		<hbox>
			<group text="groups.sidechain" vexpand="false" ipadding="0">
				<hbox bg.color="bg_schema">
					<!-- Setup -->
					<vbox>
						<label text="labels.setup" pad.h="6" pad.v="4" fill="false" vreduce="true"/>
						<ui:with pad.h="6" pad.b="4" hfill="true">
							<combo id="sct" pad.t="4"/>
							<combo id="scm" />
							<combo id="scs" visibility="!:ssplit" />
							<combo id="sscs" visibility=":ssplit" />
							<button id="scl" height="22" text="labels.listen" ui:inject="Button_cyan"/>
						</ui:with>
					</vbox>

					<!-- Lookahead/Reactivity -->
					<vbox width="90">
						<label text="labels.sc.reactivity" vreduce="true"/>
						<knob id="scr" scolor="kscale" size="20"/>
						<value id="scr" sline="true"/>

						<label text="labels.sc.lookahead" vreduce="true"/>
						<knob id="sla" scolor="kscale" size="20"/>
						<value id="sla" sline="true"/>
					</vbox>

					<!-- Filters -->

					<!-- HPF -->
					<vbox>
						<label text="labels.flt.HPF" pad.h="6" pad.v="4" fill="false" vreduce="true"/>
						<combo id="shpm" pad.h="6" bright="(:shpm igt 0) ? 1 : 0.75" bg.bright="(:shpm igt 0) ? 1 : :const_bg_darken"/>
						<knob id="shpf" size="40" scolor="(:shpm igt 0) ? 'kscale' : 'kscale_inactive'" bg.bright="(:shpm igt 0) ? 1 : :const_bg_darken"/>
						<value id="shpf" sline="true" bright="(:shpm igt 0) ? 1 : 0.75" bg.bright="(:shpm igt 0) ? 1 : :const_bg_darken"/>
					</vbox>
					<!-- TODO: Check this upstream changes -->
					<!-- <cell rows="2"><knob id="scp" scolor="kscale"/></cell>
					<cell rows="2"><knob id="scr" scolor="kscale"/></cell>
					<cell rows="2"><knob id="sla" scolor="kscale"/></cell>
					<cell rows="3">
						<vbox fill="false" spacing="1">
							<ui:with pad.h="6" pad.b="4" hfill="true">
								<combo id="sct" pad.t="4"/>
								<combo id="scm"/>
								<combo id="scs" visibility="!:ssplit"/>
								<combo id="sscs" visibility=":ssplit"/>
								<button id="scl" height="22" text="labels.listen" ui:inject="Button_cyan"/>
							</ui:with>
						</vbox>
					</cell>
					<combo id="shpm" pad.h="6" bright="(:shpm igt 0) ? 1 : 0.75" bg.bright="(:shpm igt 0) ? 1 : :const_bg_darken"/>
					<combo id="slpm" pad.h="6" bright="(:slpm igt 0) ? 1 : 0.75" bg.bright="(:slpm igt 0) ? 1 : :const_bg_darken"/> -->
					<!-- TODO: /Check this upstream changes -->

					<!-- LPF -->
					<vbox>
						<label text="labels.flt.LPF" pad.h="6" pad.v="4" fill="false" vreduce="true"/>
						<combo id="slpm" pad.h="6" bright="(:slpm igt 0) ? 1 : 0.75" bg.bright="(:slpm igt 0) ? 1 : :const_bg_darken"/>
						<knob id="slpf" size="40" scolor="(:slpm igt 0) ? 'kscale' : 'kscale_inactive'" bg.bright="(:slpm igt 0) ? 1 : :const_bg_darken"/>
						<value id="slpf" sline="true" bright="(:slpm igt 0) ? 1 : 0.75" bg.bright="(:slpm igt 0) ? 1 : :const_bg_darken"/>
					</vbox>
				</hbox>
			</group>
			<hsep pad.h="2" bg.color="bg"/>

			<group text="groups.plug.compressor" vexpand="false" hexpand="true" ipadding="0">

				<vbox rows="5" cols="14" spacing="0" bg.color="bg_schema">

					<hbox>
						<!-- Mode -->
						<vbox>
							<label text="labels.mode" pad.h="6" pad.v="4" vreduce="true"/>

							<vbox>
								<combo id="cm" pad.h="6" pad.v="4"/>
								<vbox visibility=":cm ine 0">
									<label text="labels.boost" />
									<knob id="bth" scolor="kscale" visibility=":cm ine 2"/>
									<value id="bth" detailed="false" visibility=":cm ine 2"/>
									<knob id="bsa" scolor="kscale" visibility=":cm ieq 2"/>
									<value id="bsa" detailed="false" visibility=":cm ieq 2"/>
								</vbox>
							</vbox>

							<void vexpand="true" />
						</vbox>

						<vbox>
							<label text="labels.threshold" vreduce="false"/>
							<knob id="al" size="50" scolor="attack_1"/>
							<align valign="-1" pad.t="2">
								<value id="al" detailed="true" sline="true" />
							</align>
						</vbox>

						<vbox>
							<label text="labels.ratio" vreduce="false"/>
							<knob id="cr" size="50" scolor="kscale"/>
							<!-- <knob id="rrl" size="20" scolor="kscale"/> -->
							<align valign="-1" pad.t="2">
								<hbox fill="false">
									<value id="cr" detailed="false"/>
									<label text=":"/>
									<label text="1"/>
								</hbox>
							</align>
						</vbox>

						<vbox width.min="75" >
							<vbox>
								<label text="labels.attack" />
								<knob id="at" size="30" ssize="3" scolor="pink" pad.t="4" />
								<value id="at" sline="true" pad.h="6" pad.v="4" width.min="48" />
							</vbox>

							<vbox>
								<label text="labels.hold" pad.h="6" pad.v="4" vreduce="true" />
								<knob id="hold" size="20" pad.h="6" />
								<value id="hold"/>
							</vbox>

							<vbox>
								<label text="labels.release"/>
								<knob id="rt" size="30" ssize="3" scolor="release_1" pad.t="4" />
								<!-- TODO: It will be convenient if the value also displays fraction time in ms -->
								<value id="rt" sline="true" pad.h="6" pad.v="4" width.min="48" />
								<!-- <frac visibility=":rm_${i} ieq 1" id="rdf_${i}" den.id="rdd_${i}" color="green"/> -->
								<!-- <button text="BPM" id="rm_${i}" ui:inject="Button_cyan" /> -->
							</vbox>
						</vbox>

						<vbox>
							<label text="labels.signal.wet"/>
							<knob id="cwt" scolor="wet" size="30" />
							<value id="cwt" sline="true" />

							<button id="amk" pad.h="6" height="18" width="70" text="Auto gain" ui:inject="Button_orange_8"/>

							<label text="labels.signal.dry"/>
							<knob id="cdr" scolor="dry" size="30" />
							<value id="cdr" sline="true" />
						</vbox>
					</hbox>

					<!-- <hbox>
						<ui:with pad.h="6" pad.v="4">

							<label visibility="false" text="labels.knee" vreduce="true"/>
